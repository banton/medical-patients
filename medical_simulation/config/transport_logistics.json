  "transport_time_calculation": {
    "base_formula": "distance_km / speed_kmh",
    "modifiers": {
      "terrain": {
        "road": 1.0,
        "highway": 0.8,
        "mixed": 1.2,
        "forest": 1.5,
        "arctic": 1.8,
        "urban": 1.3,
        "air": 0.5
      },
      "contested": {
        "true": 2.0,
        "false": 1.0
      },
      "weather": {
        "clear": 1.0,
        "rain": 1.2,
        "snow": 1.5,
        "storm": 2.0,
        "extreme": 3.0
      },
      "mass_casualty": {
        "active": 1.5,
        "inactive": 1.0
      }
    }
  },
  "deterioration_during_transport": {
    "ground": {
      "multiplier": 1.2,
      "additional_per_hour": 2
    },
    "air": {
      "multiplier": 1.0,
      "additional_per_hour": 0
    },
    "uncontrolled": {
      "multiplier": 2.0,
      "additional_per_hour": 5
    }
  },
  "csu_operations": {
    "functions": [
      "triage_reassessment",
      "stabilization",
      "evacuation_prioritization",
      "resource_allocation",
      "communication_hub"
    ],
    "decision_rules": {
      "hold_for_batch_evacuation": true,
      "minimum_batch_size": 4,
      "maximum_hold_time_hours": 4,
      "priority_override_for_t1": true
    }
  },
  "overflow_transport_rules": {
    "bypass_full_facility": {
      "enabled": true,
      "bypass_conditions": {
        "facility_full": true,
        "t1_patient": true,
        "next_facility_available": true
      }
    },
    "forced_evacuation": {
      "trigger": "facility_at_capacity",
      "selection": "longest_waiting_stable",
      "destination": "next_available_facility"
    },
    "died_in_transit_tracking": {
      "categories": [
        "died_awaiting_transport",
        "died_during_ground_evacuation",
        "died_during_air_evacuation",
        "died_at_csu",
        "died_bypassing_full_facility"
      ]
    }
  }
